[user]
	email = kaisea.rpi@gmail.com
	name = Andrew Kaiser
[push]
	default = current
[fetch]
	prune = true
[core]
	autocrlf = false
	preloadindex = true
	symlinks = false
	pager = diff-so-fancy | less --tabs=4 -RFX
[log]
	decorate = short
[http]
	sslVerify = false
[oh-my-zsh]
	hide-dirty = 1
	hide-status = 1
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[diff]
	word-diff = color
[alias]
	df = diff --word-diff=plain
